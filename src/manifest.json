{
  "name": "Harvest hour balance",
  "description": "Shows the current hour balance for logged in user",
  "version": "1.2.0",
  "manifest_version": 2,
  "permissions": [ "tts" ],
  "content_scripts": [
    {
      "matches": ["https://*.harvestapp.com/*"],
      "css": ["styles.css"],
      "js": ["jquery-2.1.1.min.js","localforage.min.js","mustache.min.js","moment.min.js","harvest.js"]
    }
  ],
  "web_accessible_resources": [
    "main-panel.html",
    "settings-panel.html"
  ]
}
